---
interface Props {
	title: string;
}

const { title } = Astro.props;

import "~/styles/index.css"
import "aos/dist/aos.css";

---

<!doctype html>
<html lang="es">
	<head>
		<meta charset="UTF-8" />
        <meta name="description" content="Descubre Asegura-Te, la correduría de Seguros en Las Palmas que te brinda confianza y seguridad. Protege lo que más importa. ¡Contáctanos hoy!">
		<meta name="viewport" content="width=device-width" />
        <link rel="icon" type="image/png" sizes="32x32" href="/favicon-32x32.png">
        <link rel="icon" type="image/png" sizes="16x16" href="/favicon-16x16.png">
		<meta name="generator" content={Astro.generator} />
		<title>{title}</title>
		 <!-- --FONTAWESOME-- -->
		 <script src="https://kit.fontawesome.com/e044e0c03c.js" crossorigin="anonymous"></script>
		 
	</head>
	<body>
		<slot />
		<script>
			import { aosInit } from "~/scripts/aos";
			aosInit();
		</script>
	</body>
</html>